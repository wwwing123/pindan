<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93" wx:if="{{type == 'confirm' ? true : false}}"></icon>
            <icon type="success" size="93" color="rgb(43,52,60)" wx:else></icon>           
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{msg}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="back" wx:if="{{type == 'confirm' ? true : false}}">完成</button>
                <button class="weui-btn" type="primary" bindtap="back" style="background:#2B343C;" wx:else>完成</button>
            </view>
        </view>
    </view>
</view>